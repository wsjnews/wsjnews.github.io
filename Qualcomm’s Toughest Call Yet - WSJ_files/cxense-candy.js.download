close2x.png" width="32px" height="32px" title="Close"/>\n        </div>\n        <div tmp:class="cx-scrim-creative">\n            <a tmp:id="{{cX.clickTracker(ad.clickUrl)}}" tmp:href="{{ad.creative.destinationUrl}}" tmp:target="_blank">\n                <div class="eight"><img tmp:src="{{defaultCreative}}" tmp:title="" tmp:alt="" ></div>\n                <div class="four"><img tmp:src="{{smallCreative}}" tmp:title="" tmp:alt="" ></div>\n            </a>\n\n            <!--% if(trackingPixel){ %-->\n            <div><img tmp:src="{{trackingPixel}}" title="" alt="" width="0" height="0"></div>\n            <!--% } %-->\n\n        </div>\n\n    </div>\n</div>\n<!-- TEMPLATE_END -->',renderingTemplates["scrim.style"]='<!-- STYLE_START -->\n    <style type="text/css">\n        #scrimTemplate.template {display:none;}\n        #cx-scrim{width:100%; height:100%; position:fixed;  z-index: 1000;top:0; left:0; display:none}\n        #cx-scrim-wrapper{position:fixed;top:50%; left:50%;margin-left:-310px; margin-top:-232px;width:620px; height:432px;opacity:1;background:transparent}\n        #cx-scrim-wrapper .close-btn{position:relative;background:transparent; cursor: pointer; width:32px; height:32px;left: 603px;top: 15px;}\n        .cx-scrim-creative, .cx-scrim-creative img{width:620px; height:400px;clear:right}\n        .cx-scrim-creative .four{display:none}\n\n        @media all and (max-width: 635px) and (min-width:120px) {\n            .cx-scrim-creative .eight{display:none}\n            .cx-scrim-creative .four{display:block}\n            #cx-scrim-wrapper{position:fixed;top:50%; left:50%;;width:300px; height:282px; margin-left: -150px;margin-top: -160px;}\n            .cx-scrim-creative, .cx-scrim-creative img{width:300px;height:250px;}\n            #cx-scrim-wrapper .close-btn{left: 268px;top: -4px;}\n        }\n    </style>\n    <!-- STYLE_END -->',renderingTemplates.signin='<!-- TEMPLATE_START -->\n<div id="signinReminderTemplate" class="template">\n\n    <!--% var ad =