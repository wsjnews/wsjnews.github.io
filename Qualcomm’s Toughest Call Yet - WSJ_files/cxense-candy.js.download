">\n    <!--%\n    var ad = data;\n    var trackingPixel = cxad.getContent(ad, \'Tracking Pixel\');\n    var signinLink = "https://accounts.barrons.com/login?opts=0&target=" + document.location.href ;\n    var passDuration = (cxenseOffers.coverState.sevenDays) ? \'7 days\' : \'48 hours\';\n    var wrapperClass = (cxenseOffers.coverState.sevenDays) ? \'cx-member-share\' : \'cx-email-offer\';\n    %-->\n    <div tmp:id="cx-daypass-scrim">\n        <div tmp:id="cx-daypass" class="cx-daypass {{wrapperClass}}">\n            <div tmp:class="close-btn">\n                <svg xmlns="http://www.w3.org/2000/svg" width="14.7" height="14.7" viewBox="0 0 14.7 14.7">\n                    <path d="M14.7.7L14 0 7.4 6.6.7 0 0 .7l6.6 6.7L0 14l.7.7 6.7-6.6 6.6 6.6.7-.7-6.6-6.6"></path>\n                </svg>\n            </div>\n            <div tmp:class="cx-daypass-creative">\n                <h2 class="cx-email-offer">Enjoy guest access to<br/> Barrons.com for {{passDuration}}.</h2>\n                <h2 class="cx-member-share">Enjoy the gift of a {{passDuration}} guest pass courtesy of a Barron\'s member.</h2>\n                <h3 class="cx-member-share">Enjoy a {{passDuration}} guest pass courtesy of a Barron\'s member.</h3>\n                <h5>No credit card required.</h5>\n                <p tmp:class="cx-daypass-notes">\n                    Access Barrons.com for up to {{passDuration}} on this device only, provided you don’t clear cookies.\n                    By clicking Submit, you agree to our <a target="_top" href="http://www.dowjones.com/privacy-policy/">Privacy Policy</a>\n                    and <a target="_top" href="http://new.dowjones.com/about/cookies-policy/">Cookie Policy</a>, and understand you’ll\n                    receive promotion from Dow Jones and affiliates.\n                </p>\n                <div tmp:class="cx-daypass-form empty">\n                    <form tmp:id="daypass-form">\n                        <div tmp:id="cx-daypass-entry-box" tmp:class=