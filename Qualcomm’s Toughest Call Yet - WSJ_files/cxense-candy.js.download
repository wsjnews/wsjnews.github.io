erlay img{margin:0;padding:0;border:none;}\n        #cx-overlay .close-btn{ position:absolute;left: 880px; padding:15px; width: 32px; height:32px; cursor:pointer; }\n\n        /*        .at16units #cx-articlecover .close-btn {left: 875px;}\n                .at12units #cx-articlecover .close-btn{left: 560px;}\n          */      .at8units #cx-articlecover .close-btn {padding:15px 15px 5px;}\n\n        .at8units .sml, .at12units .sml, .at16units .sml, .at4units .lrg {display:none;}\n        .cx-overlay-creative .lrg img {width:940px;}\n\n        .at16units #cx-article-overlay {width:1020px;}\n        .at16units .cx-overlay-creative{text-align:left}\n\n        .at12units #cx-article-overlay {width:870px;}\n        .at12units .cx-overlay-creative .lrg img {width:620px;}\n\n        .at8units #cx-article-overlay {width:620px;}\n        .at8units .cx-overlay-creative .lrg img {width:620px;}\n\n        .at4units #cx-article-overlay {width: 100%;}\n        .at4units #cx-overlay {position: absolute;width: auto;height: 640px;background-color: #384D60;margin-left: 0;padding: 0;top: 100px;left: 0;right: 0;}\n        .at4units .cx-overlay-creative .sml img { width:100%; height: auto; }\n        .at4units #cx-articlecover .close-btn {padding:20px 15px 5px;}\n        .at4units #cx-overlay .close-btn{right: 0; left:initial}\n\n\n        /* DayPass & MemberPass CSS */\n        #cx-daypass-scrim {background: rgba(245, 245, 240,0.9);padding: 0;margin:0;position: relative;}\n        .cx-daypass-creative{padding-top:20px}\n        .cx-daypass {background-color:#203140;width:940px;height: 680px;padding: 0;margin: 0;position: relative;text-align:center;box-shadow: 0 2px 10px 0 rgba(147,147,147,0.35);}\n        #cx-articlecover .cx-daypass .close-btn{position: absolute;right: 0;top: 0;padding:25px 25px 5px;cursor:pointer;}\n        .cx-daypass svg{fill:#ffffff;width: 16px;height: 16px;}\n        .cx-daypass .close-btn:hover svg{fill:#2fafcd}\n\n        div.cx-email-offer .cx-member-share, di