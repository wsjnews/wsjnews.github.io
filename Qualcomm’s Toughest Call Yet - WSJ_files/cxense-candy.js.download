!--%\n    var ad = data;\n    var defaultCreative = cxad.getImage(ad, '620 default');\n    var smallCreative = cxad.getImage(ad, '300 default');\n    var trackingPixel = cxad.getContent(ad, 'Tracking Pixel');\n\n    //Dynamic EDU Offer-specific\n    var experience = cxad.getContent(ad, 'Experience');\n    var eduOffer = '';\n    if(experience === 'educationActivation'){\n        //Add in a fallback in case a Dynamic EDU OFfer is returned without a message.\n        var offerDescription = cxad.getContent(ad, 'Offer Description', \"Your Institution sponsors WSJ Access for all their Students and Staff.\");\n        eduOffer = 'eduOffer';\n    }\n\n    %-->\n    <div tmp:id=\"cx-scrim-overlay\">\n        <div tmp:id=\"cx-scrim\">\n            <div tmp:class=\"close-btn\">\n"+'                <img tmp:src="https://s.wsj.net/img/cxense/icon-close2x.png" width="32px" height="32px" title="Close"/>\n            </div>\n            <div tmp:class="cx-scrim-creative {{eduOffer}}">\n                <a tmp:id="{{cX.clickTracker(ad.clickUrl)}}" tmp:href="{{ad.creative.destinationUrl}}" tmp:target="_blank">\n                    <div class="eight">\n                        <!--% if(eduOffer){ %-->\n                        <span>{{offerDescription}}</span>\n                        <div class="cta-wrap">\n                            <div class="action-reminder">Click Below to Start Reading</div>\n                            <div class="action-reminder-triangle"></div>\n                            <div class="call-to-action">Activate Now</div>\n                            <div class="no-payment">No Payment Required</div>\n                        </div>\n                        <!--% } %-->\n                        <img tmp:src="{{defaultCreative}}" tmp:title="" tmp:alt="" >\n                    </div>\n                    <div class="four">\n                        <!--% if(eduOffer){ %-->\n                        <span>{{offerDescription}}</span>\n                        <div class="cta-wrap">\n                            <div class="action-reminder">Click Below to Start Reading</div>\n                            <div class="action-reminder-triangle"></div>\n                            <div class="call-to-action">Activate Now</div>\n                            <div class="no-payment">No Payment Required</div>\n                        </div>\n                        <!--% } %-->\n                        <img tmp:src="{{smallCreative}}" tmp:title="" tmp:alt="" >\n                    </div>\n                </a>\n\n                <!--% if(trackingPixel){ %-->\n                <div><img tmp:src="{{trackingPixel}}" title="" alt="" width="0" height="0"></div>\n                <!--% } %-->\n            </div>\n        </div>\n    </div>\n</div>\n\n<div id="daypassTemplate" class="template">\n    <!--%\n    var ad = data;\n    var trackingPixel = cxad.getContent(ad, \'Tracking Pixel\');\n    var signinLink = "https://id.wsj.com/access/509b1a086458232f6e000002/latest/login_standalone.html?url=" + document.location.href;\n    var passDuration = (cxenseOffers.coverState.sevenDays) ? \'7 days\' : \'24 hours\';\n    var readAcross = cxenseOffers.coverState.readAcross || cxenseOffers.coverState.propensityGuestPass;\n    var davos2018GuestPass = cxenseOffers.coverState.davos2018GuestPass;\n    var wrapperClass = (cxenseOffers.coverState.sevenDays || davos2018GuestPass) ? \'cx-member-share\' : \'cx-email-offer\';\n    if (cxenseOffers.coverState.sevenDays && !readAcross && !davos2018GuestPass) {\n      wrapperClass += \' cx-seven-days\';\n    }\n    if (readAcross) {\n      wrapperClass += \' cx-read-across\';\n    }\n    if (davos2018GuestPass) {\n      wrapperClass += \' cx-davos2018\';\n    }\n    %-->\n    <div tmp:id="cx-daypass-scrim">\n    <div tmp:id="cx-daypass" class="cx-daypass {{wrapperClass}}">\n        <div tmp:class="close-btn">\n            <img tmp:src="https://s.wsj.net/img/cxense/icon-close2x.png" width="32px" height="32px" title="Close"/>\